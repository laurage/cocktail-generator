<h1>Cocktails</h1>

<% @cocktails.each do |cocktail| %>
  <p><%= link_to cocktail.name, cocktail_path(cocktail) %></p>
  <p><%= link_to "Delete", cocktail_path(cocktail), method: :delete %> - <%= link_to "Edit", edit_cocktail_path(cocktail) %></p>
<%end%>
</br>
<%= link_to "Add a cocktail", new_cocktail_path %>


